                    />

                <TextView
                    android:id="@+id/trans_type1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="65dp"
                    android:text="2212"
                    />

                <TextView
                    android:id="@+id/bt_institutiona_upload2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="35dp"
                    android:background="@drawable/mine_jiantou"
                    android:gravity="center"
                    android:textColor="@color/blue"
                    android:visibility="gone"
                     />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bt_qukuailian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                >


                <TextView
                    android:id="@+id/qukuailian_message"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/margin_20dp"
                    android:text="@string/mine_enterpriseinformation"
                    android:textColor="@color/dark"
                    android:textSize="20dp" />

                <TextView
                    android:id="@+id/bt_qukuanlian"
                    android:layout_width="85dp"
                    android:layout_height="35dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:background="@drawable/enterprise_upload_small_write"
                    android:gravity="center"
                    android:text="@string/mine_enterpriseinformation_check"
                    android:textColor="@color/blue" />

                <TextView
                    android:id="@+id/bt_look_qushow"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="25dp"
                    android:inputType="textMultiLine"
                    android:paddingLeft="@dimen/margin_150dp"
                    android:singleLine="false"
                    android:text="askldad"
                    android:visibility="gone" />

            </RelativeLayout>
        </LinearLayout>


    </LinearLayout>
</ScrollView>