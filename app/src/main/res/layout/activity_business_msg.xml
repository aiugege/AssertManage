                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="35dp"
                    android:background="@drawable/mine_jiantou"
                    android:gravity="center"
                    android:textColor="@color/blue"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/swf_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_35dp"
                    android:textColor="@color/red" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bt_institutiona"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/margin_10dp"
                android:layout_marginTop="25dp"

                >

                <TextView
                    android:id="@+id/tv_institutiona_information"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/margin_20dp"
                    android:text="@string/Institutional_message"
                    android:textColor="@color/dark"
                    android:textSize="20dp" />

                <TextView
                    android:id="@+id/put_institutiona_information"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="150dp"
                    android:text="@string/mine_enterpriseinformation_will_finish"
                    android:textSize="20dp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/trans_type2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
