                android:layout_marginTop="25dp">


                <TextView
                    android:id="@+id/et_business_license"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/margin_20dp"
                    android:text="@string/et_business_license"
                    android:textColor="@color/dark"
                    android:textSize="20dp" />

                <TextView
                    android:id="@+id/put_business_license_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="150dp"
                    android:text="待完善"
                    android:textSize="20dp"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/bt_enterprice_upload"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="35dp"
                    android:src="@drawable/mine_jiantou"
                    android:textColor="@color/blue"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/yingye_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_35dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/bt_reaset_password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp">


